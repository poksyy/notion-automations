# Notion API Configuration
# Copy this file to .env and fill in your values

# Your Notion integration token (starts with secret_)
NOTION_TOKEN=secret_your_token_here

# Database IDs (found in the URL when you open the database)
# Example URL: https://notion.so/workspace/1234567890abcdef1234567890abcdef?v=...
#                                         ^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^
#                                         This is the database ID

DAYS_DATABASE_ID=your_days_database_id_here
DAILY_LOG_DATABASE_ID=your_daily_log_database_id_here
